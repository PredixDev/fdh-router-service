{
    "$schema": "http://json-schema.org/draft-03/schema",
    "title": "AssetTag",
    "description": "AssetTag which is the link between Asset and Tag",
    "type": "object",
    "required": false,
    "additionalProperties": false,
    "properties": {
        "tagUri": {
            "description": "Tag uri",
            "type": "string",
            "required": false
        },
        "sourceTagId": {
            "description": "Time series tag id",
            "type": "string",
            "required": false
        },
        "dataSource": {
            "description": "data source",
            "type": "string",
            "required": true,
            "default": false
        },
        "isManual": {
            "description": "Indicate if data can be manually input",
            "type": "boolean",
            "required": true,
            "default": false
        }
    }
}